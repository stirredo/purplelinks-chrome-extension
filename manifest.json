{
  "name": "HelloChromeBrowser",
  "version": "0.0.1",
  "manifest_version": 2,
  "page_action": {
    "default_icon": "images/logo.png",
    "default_title": "Hello Chrome Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}